.container {
  font-size: 12px;
  display: inline-block;
  position: relative;
  top: -2px;
}

.setting {
  font-size: 16px;
  &:hover {
    color: #d9007e;
  }
}

.tcbModal {
  .tcbTable {
    font-size 12px;
    border-collapse: collapse;
    width: 100%;
  }
  th {
    padding: 3px 6px;
    border: solid #eee 1px;
    background-color: #eee;
    text-align: center;
  }
  td {
    &:first-child {
      text-align left;
    }
    border: solid #eee 1px;
    padding: 3px 6px;
    text-align: center;
    :global(.ant-form-item) {
      margin: 0;
    }
  }
}