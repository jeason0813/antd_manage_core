.container {
  h2 {
    text-align: center;
  }

  ul {
    color: rgba(0, 0, 0, 0.54);
    margin: 10px auto;
    list-style: disc;
    font-size: 14px;
  }
  .key {
    text-align: center;
    a {
      font-size: 14px;
      display: block;
      margin-bottom: 10px;
    }
    div{
      border: 1px dotted #ccc;
      padding: 10px 0;
      display: block;
      font-size: 14px;
      font-weight: bold;
      margin-bottom: 10px;
    }
  }
  .qr {
    text-align: center;
    div {
      padding: 54px;
    }
    a {
      font-size: 14px;
      display: block;
      margin-bottom: 10px;
      margin-top: 5px;
    }
    img {
      width: 166px;
      height: 166px;
      margin: 10px auto 0;
      display: block;
    }
  }
  .next {
    float: right;
  }
}

.download-qr {
  :global(.ant-popover-inner-content) {
    text-align: center;
  }
}